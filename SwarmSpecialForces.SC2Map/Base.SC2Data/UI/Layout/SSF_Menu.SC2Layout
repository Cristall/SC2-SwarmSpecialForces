	</Frame>

<!-- Actual Frame for all Windows, pretty much just a compilation of everything above-->
	<Frame type="Frame" name="GameUI/UIContainer/FullscreenUpperContainer" file="GameUI">
		<Frame type="Frame" name="SSF_CustomUI">
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
			<Anchor side="Bottom" relative="$parent" pos="Min" offset="830"/>
			<Visible val="false"/>

			<StateGroup name="NotificationState">
				<DefaultState val="True"/>
				<State name="False">
					<When type="StateGroup" frame="$this/Achievements" operator="Equal" NotificationState="False"/>
					<When type="StateGroup" frame="$this/Speedruns" operator="Equal" NotificationState="False"/>
				<!-- these frames do no have an notification state yet so exlude them from the condition
					<When type="StateGroup" frame="$this/Stats" operator="Equal" NotificationState="False"/>
					<When type="StateGroup" frame="$this/Votekick" operator="Equal" NotificationState="False"/>
					<When type="StateGroup" frame="$this/Options" operator="Equal" NotificationState="False"/>-->
					<Action type="SetProperty" frame="$this/MenuOpener/Notification" Visible="False"/>
				</State>
				<State name="True">
					<Action type="SetProperty" frame="$this/MenuOpener/Notification" Visible="True"/>
				</State>
			</StateGroup>

			<Frame type="Frame" name="MenuOpener" template="SSF_Menu/MenuOpenerTemplate"></Frame>
			<Frame type="Frame" name="Menu" template="SSF_Menu/MenuTemplate"></Frame>

			<Frame type="Frame" name="Stats" template="SSF_Menu/StatsTemplate"></Frame>
			<Frame type="Frame" name="Achievements" template="SSF_Menu/AchievementTemplate"></Frame>
			<Frame type="Frame" name="Speedruns" template="SSF_Menu/SpeedrunsTemplate"></Frame>
			<Frame type="Frame" name="Votekick" template="SSF_Menu/VotekickTemplate"></Frame>
			<Frame type="Frame" name="Options" template="SSF_Menu/OptionsTemplate"></Frame>
		</Frame>
	</Frame>
</Desc>
