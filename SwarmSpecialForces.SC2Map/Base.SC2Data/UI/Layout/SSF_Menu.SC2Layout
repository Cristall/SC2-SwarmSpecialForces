<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
<!-- general constants -->
	<Constant name="Border" val="40"/>
	<Constant name="BorderNegative" val="-40"/>

	<Constant name="NotificationImage" val="Assets/Textures/exclamation.dds"/> <!-- size is 64x128 -->
	<Constant name="NotificationColor" val="ff0000"/>
	<!-- alternatives: Assets/Textures/ui_replay_icon_notsaved.dds (44x40), ui_battlenet_glue_chatpanel_filternotification.dds-->

<!-- General Templates -->

	<Frame type="Button" name="ButtonTemplate" template="TriggerDialogFrame/TriggerControlButtonTemplate">
		<Style val="aaDialogItemStyle"/>
	</Frame>
	<Frame type="Label" name="LabelTemplate" template="TriggerDialogFrame/TriggerControlLabelTemplate">
		<Style val="aaDialogItemStyle"/>
	</Frame>
	<Frame type="Image" name="BackgroundImageTemplate">
		<Anchor relative="$parent"/>
		<Texture val="@@UI/TriggerDialogFrameBackground" layer="0"/>
		<TextureType val="Border" layer="0"/>
		<Tiled val="true" layer="0"/>
	</Frame>

	<Frame type="Button" name="CloseButtonTemplate" template="SSF_Menu/ButtonTemplate">
		<Text val="@Trig/CloseButton"/>
		<Anchor side="Right" relative="$parent" pos="Max" offset="#BorderNegative"/>
		<Anchor side="Top" relative="$parent" pos="Min" offset="#Border"/>
		<Height val="50"/>
		<Width val="50"/>

		<Animation name="Hide">
			<Event event="OnClick" action="Reset,Play" frame="$this"/>
			<Controller type="Visibility" end="Pause" frame="$parent">
				<Key type="Bool" time="0.0" value="false"/>
			</Controller>
		</Animation>
		<Shortcut val="Escape"/>
	</Frame>

	<Frame type="Image" name="NotificationImageTemplate">
		<Anchor side="Bottom" relative="$parent" pos="Max" offset="-7"/>
		<Anchor side="Right" relative="$parent" pos="Max" offset="-7"/>
		<Width val="16"/>
		<Height val="32"/>
		<Texture val="#NotificationImage"/>
		<Visible val="False"/>
		<Color val="ff0000"/>
		<RenderPriority val="600"/>
	</Frame>

	<Frame type="Image" name="NotificationImageInButtonTemplate" template="SSF_Menu/NotificationImageTemplate">
		<Anchor side="Top" relative="$parent" pos="Min" offset="7"/>
		<Anchor side="Right" relative="$parent" pos="Max" offset="-5"/>
	</Frame>

	<Frame type="Image" name="NotificationImageLowestLevelTemplate" template="SSF_Menu/NotificationImageTemplate">
		<Animation name="Shown">
			<Event event="OnMouseEnter" action="Reset,Play" frame="$parent/Icon"/>
			<Controller type="State" end="Pause" frame="$parent" stateGroup="NotificationState">
				<Key type="Identifier" time="0.0" value="False"/>
			</Controller>
		</Animation>
	</Frame>

	<Frame type="Image" name="NotificationImageInAchievementsTemplate" template="SSF_Menu/NotificationImageLowestLevelTemplate">
		<Anchor side="Bottom" relative="$parent" pos="Max" offset="-9"/>
		<Anchor side="Right" relative="$parent" pos="Max" offset="-9"/>
	</Frame>

    <Frame type="CheckBox" name="CheckBoxTemplate" template="TriggerDialogFrame/TriggerControlCheckBoxTemplate">
	</Frame>
	

	<Frame type="Pulldown" name="PulldownTemplate" template="TriggerDialogFrame/TriggerControlPulldownTemplate">
	</Frame>
	

<!-- Perma Menu Open button-->
	<Constant name="MenuOpenerHeight" val="82"/>
	<Constant name="MenuOpenerWidth" val="250"/>

	<Frame type="Frame" name="MenuOpenerTemplate">
		<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
		<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

		<Width val="#MenuOpenerWidth"/>
		<Height val="#MenuOpenerHeight"/>

		<Frame type="Image" name="BackgroundImage" template="SSF_Menu/BackgroundImageTemplate"></Frame>
		
		<Frame type="Image" name="Notification" template="SSF_Menu/NotificationImageInButtonTemplate">
			<Anchor side="Right" relative="$parent/OpenButton" pos="Max" offset="-7"/>
		</Frame>	

		<Frame type="Button" name="OpenButton" template="SSF_Menu/ButtonTemplate">
			<Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="-25"/>
			<Anchor side="Top" relative="$parent" pos="Min" offset="16"/>
			<Height val="50"/>


			<Text val="@Trig/MenuOpener/Text"/>
			<Animation name="ShowMenu">
				<Event event="OnClick" action="Reset,Play" frame="$this"/>
				<Controller type="Visibility" end="Pause" frame="$parent/$parent/Menu">
					<Key type="Bool" time="0.0" value="true"/>
				</Controller>
			</Animation>
		</Frame>
		<Frame type="Button" name="CloseButton" template="SSF_Menu/ButtonTemplate">
			<Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="-25"/>
			<Anchor side="Top" relative="$parent" pos="Min" offset="16"/>
			<Height val="50"/>

			<Text val="@Trig/MenuOpener/Text"/>
			<Animation name="HideMenu">
				<Event event="OnClick" action="Reset,Play" frame="$this"/>
				<Controller type="Visibility" end="Pause" frame="$parent/$parent/Menu">
					<Key type="Bool" time="0.0" value="false"/>
				</Controller>
			</Animation>
		</Frame>
	</Frame>

<!-- Menu -->
	<Constant name="MenuHeight" val="342"/>
	<Constant name="MenuWidth" val="250"/>
	<Constant name="MenuButtonHeight" val="50"/>
	<Constant name="MenuButtonGap" val="2"/>
	<Constant name="CopyText" val="Discord link"/>
	<Constant name="CopyLink" val="https://discord.gg/KwxNAJ2"/>

	<Frame type="Frame" name="MenuTemplate">

		<Anchor side="Bottom" relative="$parent/MenuOpener" pos="Min" offset="-2"/>
		<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
		<Height val="#MenuHeight"/>
		<Width val="#MenuWidth"/>

		<Visible val="false"/>

		<StateGroup name="Visiblity">
			<DefaultState val="Hidden"/>
			<State name="Hidden">
				<When type="Property" frame="$this" operator="Equal" Visible="false"/>
				<Action type="SetProperty" frame="$parent/MenuOpener/CloseButton" Visible="false"/>
				<Action type="SetProperty" frame="$parent/MenuOpener/OpenButton" Visible="true"/>
			</State>
			<State name="Shown">
				<When type="Property" frame="$this" operator="Equal" Visible="true"/>
				<Action type="SetProperty" frame="$parent/MenuOpener/CloseButton" Visible="true"/>
				<Action type="SetProperty" frame="$parent/MenuOpener/OpenButton" Visible="false"/>
				<Action type="SendEvent" frame="$this" event="CloseEverything"/>
			</State>
		</StateGroup>

		<Animation name="CloseEverything">
			<Event event="CloseEverything" action="Reset,Play" frame="$this"/>
			<Controller type="Visibility" end="Pause" frame="$parent/Stats">
				<Key type="Bool" time="0.0" value="false"/>
			</Controller>
			<Controller type="Visibility" end="Pause" frame="$parent/Achievements">
				<Key type="Bool" time="0.0" value="false"/>
			</Controller>
			<Controller type="Visibility" end="Pause" frame="$parent/Speedruns">
				<Key type="Bool" time="0.0" value="false"/>
			</Controller>
			<Controller type="Visibility" end="Pause" frame="$parent/Votekick">
				<Key type="Bool" time="0.0" value="false"/>
			</Controller>
			<Controller type="Visibility" end="Pause" frame="$parent/Options">
				<Key type="Bool" time="0.0" value="false"/>
			</Controller>
		</Animation>

		<Frame type="Button" name="HiddenCloseButton" template="SSF_Menu/CloseButtonTemplate">
			<Anchor side="Right" relative="$parent" pos="Max" offset="2000"/>
		</Frame>

		<Frame type="Image" name="BackgroundImage" template="SSF_Menu/BackgroundImageTemplate"></Frame>

		<Frame type="Button" name="Stats" template="SSF_Menu/ButtonTemplate">
			<Anchor side="Left" relative="$parent" pos="Min" offset="25"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="-25"/>
			<Anchor side="Top" relative="$parent" pos="Min" offset="16"/>
			<Height val="#MenuButtonHeight"/>
			<Text val="@Trig/Menu/Stats"/>
			
			<Animation name="Click">
				<Event event="OnClick" action="Reset,Play" frame="$this"/>
				<Controller type="Visibility" end="Pause" frame="$parent">
					<Key type="Bool" time="0.0" value="false"/>
				</Controller>
				<Controller type="Visibility" end="Pause" frame="$parent/$parent/Stats">
					<Key type="Bool" time="0.0" value="true"/>
				</Controller>
			</Animation>
		</Frame>
		<Frame type="Button" name="Achievements" template="SSF_Menu/ButtonTemplate">
			<Anchor side="Left" relative="$parent/Stats" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent/Stats" pos="Max" offset="0"/>
			<Anchor side="Top" relative="$parent/Stats" pos="Max" offset="#MenuButtonGap"/>
			<Height val="#MenuButtonHeight"/>
			<Text val="@Trig/Menu/Achievements"/>

			<Frame type="Image" name="Notification" template="SSF_Menu/NotificationImageInButtonTemplate"></Frame>

			<StateGroup name="NotificationState">
				<DefaultState val="True"/>
				<State name="False">
					<When type="StateGroup" frame="$parent/$parent/Achievements" operator="Equal" NotificationState="False"/>
					<Action type="SetProperty" frame="$this/Notification" Visible="False"/>
				</State>
				<State name="True">
					<Action type="SetProperty" frame="$this/Notification" Visible="True"/>
				</State>
			</StateGroup>
			
			<Animation name="Click">
				<Event event="OnClick" action="Reset,Play" frame="$this"/>
				<Controller type="Visibility" end="Pause" frame="$parent">
					<Key type="Bool" time="0.0" value="false"/>
				</Controller>
				<Controller type="Visibility" end="Pause" frame="$parent/$parent/Achievements">
					<Key type="Bool" time="0.0" value="true"/>
				</Controller>
			</Animation>
		</Frame>
		<Frame type="Button" name="Speedruns" template="SSF_Menu/ButtonTemplate">
			<Anchor side="Left" relative="$parent/Stats" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent/Stats" pos="Max" offset="0"/>
			<Anchor side="Top" relative="$parent/Achievements" pos="Max" offset="#MenuButtonGap"/>
			<Height val="#MenuButtonHeight"/>
			<Text val="@Trig/Menu/Speedruns"/>

			<Frame type="Image" name="Notification" template="SSF_Menu/NotificationImageInButtonTemplate"></Frame>

			<StateGroup name="NotificationState">
				<DefaultState val="True"/>
				<State name="False">
					<When type="StateGroup" frame="$parent/$parent/Speedruns" operator="Equal" NotificationState="False"/>
					<Action type="SetProperty" frame="$this/Notification" Visible="False"/>
				</State>
				<State name="True">
					<Action type="SetProperty" frame="$this/Notification" Visible="True"/>
				</State>
			</StateGroup>
			
			<Animation name="Click">
				<Event event="OnClick" action="Reset,Play" frame="$this"/>
				<Controller type="Visibility" end="Pause" frame="$parent">
					<Key type="Bool" time="0.0" value="false"/>
				</Controller>
				<Controller type="Visibility" end="Pause" frame="$parent/$parent/Speedruns">
					<Key type="Bool" time="0.0" value="true"/>
				</Controller>
			</Animation>
		</Frame>
		<Frame type="Button" name="Votekick" template="SSF_Menu/ButtonTemplate">
			<Anchor side="Left" relative="$parent/Stats" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent/Stats" pos="Max" offset="0"/>
			<Anchor side="Top" relative="$parent/Speedruns" pos="Max" offset="#MenuButtonGap"/>
			<Height val="#MenuButtonHeight"/>
			<Text val="@Trig/Menu/Votekick"/>
			
			<Animation name="Click">
				<Event event="OnClick" action="Reset,Play" frame="$this"/>
				<Controller type="Visibility" end="Pause" frame="$parent">
					<Key type="Bool" time="0.0" value="false"/>
				</Controller>
				<Controller type="Visibility" end="Pause" frame="$parent/$parent/Votekick">
					<Key type="Bool" time="0.0" value="true"/>
				</Controller>
			</Animation>
		</Frame>
		<Frame type="Button" name="Options" template="SSF_Menu/ButtonTemplate">
			<Anchor side="Left" relative="$parent/Stats" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent/Stats" pos="Max" offset="0"/>
			<Anchor side="Top" relative="$parent/Votekick" pos="Max" offset="#MenuButtonGap"/>
			<Height val="#MenuButtonHeight"/>
			<Text val="@Trig/Menu/Options"/>
			
			<Animation name="Click">
				<Event event="OnClick" action="Reset,Play" frame="$this"/>
				<Controller type="Visibility" end="Pause" frame="$parent">
					<Key type="Bool" time="0.0" value="false"/>
				</Controller>
				<Controller type="Visibility" end="Pause" frame="$parent/$parent/Options">
					<Key type="Bool" time="0.0" value="true"/>
				</Controller>
			</Animation>
		</Frame>

		<!-- Discord link button for inside the my menu -->
		<Frame type="CopyClipboardButton" name="DiscordClipboardButton" template="SSF_Menu/ButtonTemplate">
			<Anchor side="Left" relative="$parent/Stats" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent/Stats" pos="Max" offset="0"/>
			<Anchor side="Top" relative="$parent/Options" pos="Max" offset="#MenuButtonGap"/>
			<Height val="#MenuButtonHeight"/>

			<ClipboardText val="#CopyLink"/>
			<Text val="#CopyText"/>

			<Animation name="Color">
				<Event event="OnClick" action="Restart,Play" frame="$this"/>
				<Controller type="Color" end="Reset">
					<Key type="Curve" time="0" value="255,255,255" in="Fast"/>
					<Key type="Curve" time="0.1" value="0,255,0" in="Slow"/>
					<Key type="Curve" time="5" value="0,255,0" in="Slow"/>				
				</Controller>
			</Animation>
		</Frame>
	</Frame>


<!-- Stats Window -->
	<Constant name="StatsTextRowHeight" val="28"/>
	<Constant name="StatsTextRowGap" val="0"/>
	<Constant name="StatsTextRowSpacing" val="2"/>

	<Frame type="Label" name="StatsBeforeAnchorLabelTemplate" template="SSF_Menu/LabelTemplate">
		<Anchor side="Left" relative="$parent/AnchorFrame" pos="Min" offset="0"/>
		<Anchor side="Right" relative="$parent/AnchorFrame" pos="Max" offset="0"/>
		<Height val="#StatsTextRowHeight"/>
		<Style val="aaDialogItemStyleRightMiddle"/>
	</Frame>

	<Frame type="Label" name="StatsAfterAnchorLabelTemplate" template="SSF_Menu/LabelTemplate">
		<Anchor side="Left" relative="$parent/AnchorFrame" pos="Max" offset="#StatsTextRowSpacing"/>
		<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
		<Height val="#StatsTextRowHeight"/>
		<Style val="aaDialogItemStyleLeftMiddle"/>
	</Frame>

	<!-- Creates player frames via trigger for each player and sets their position-->
	<Frame type="Frame" name="StatsPlayerTemplate">
		<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
		<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
		<Width val="300"/>
		<Height val="224"/>

		<Frame type="Frame" name="AnchorFrame">
			<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Width val="150"/>
		</Frame>

		<Frame type="Label" name="PlayerName" template="SSF_Menu/LabelTemplate">
			<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
			<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
			<Height val="#StatsTextRowHeight"/>
			<Style val="aaDialogItemStyleCentered"/>
		</Frame>

		<Frame type="Label" name="PointsLabel" template="SSF_Menu/StatsBeforeAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/PlayerName" pos="Max" offset="#StatsTextRowGap"/>
			<Text val="@Trig/Stats/Points"/>
		</Frame>
		<Frame type="Label" name="Points" template="SSF_Menu/StatsAfterAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/PointsLabel" pos="Min" offset="0"/>
		</Frame>

		<Frame type="Label" name="KillsLabel" template="SSF_Menu/StatsBeforeAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/PointsLabel" pos="Max" offset="#StatsTextRowGap"/>
			<Text val="@Trig/Stats/Kills"/>
		</Frame>
		<Frame type="Label" name="Kills" template="SSF_Menu/StatsAfterAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/KillsLabel" pos="Min" offset="0"/>
		</Frame>
		
		<Frame type="Label" name="WinsP1Label" template="SSF_Menu/StatsBeforeAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/KillsLabel" pos="Max" offset="#StatsTextRowGap"/>
			<Text val="@Trig/Stats/WinsP1"/>
		</Frame>
		<Frame type="Label" name="WinsP1" template="SSF_Menu/StatsAfterAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/WinsP1Label" pos="Min" offset="0"/>
		</Frame>
		
		<Frame type="Label" name="WinsP2Label" template="SSF_Menu/StatsBeforeAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/WinsP1Label" pos="Max" offset="#StatsTextRowGap"/>
			<Text val="@Trig/Stats/WinsP2"/>
		</Frame>
		<Frame type="Label" name="WinsP2" template="SSF_Menu/StatsAfterAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/WinsP2Label" pos="Min" offset="0"/>
		</Frame>
		
		<Frame type="Label" name="ScientistsLabel" template="SSF_Menu/StatsBeforeAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/WinsP2Label" pos="Max" offset="#StatsTextRowGap"/>
			<Text val="@Trig/Stats/Scientists"/>
		</Frame>
		<Frame type="Label" name="Scientists" template="SSF_Menu/StatsAfterAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/ScientistsLabel" pos="Min" offset="0"/>
		</Frame>
		
		<Frame type="Label" name="EssencesLabel" template="SSF_Menu/StatsBeforeAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/ScientistsLabel" pos="Max" offset="#StatsTextRowGap"/>
			<Text val="@Trig/Stats/Essences"/>
		</Frame>
		<Frame type="Label" name="Essences" template="SSF_Menu/StatsAfterAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/EssencesLabel" pos="Min" offset="0"/>
		</Frame>
		
		<Frame type="Label" name="PsiOrbsLabel" template="SSF_Menu/StatsBeforeAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/EssencesLabel" pos="Max" offset="#StatsTextRowGap"/>
			<Text val="@Trig/Stats/PsiOrbs"/>
		</Frame>
		<Frame type="Label" name="PsiOrbs" template="SSF_Menu/StatsAfterAnchorLabelTemplate">
			<Anchor side="Top" relative="$parent/PsiOrbsLabel" pos="Min" offset="0"/>
		</Frame>
	</Frame>

	<!-- Container frame for stats to use stategroups for visibility and close button-->
	<Frame type="Frame" name="StatsTemplate">
		<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
		<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
		<Width val="500"/>
		<Height val="500"/>
		<Frame type="Image" name="BackgroundImage" template="SSF_Menu/BackgroundImageTemplate"></Frame>
		<Frame type="Button" name="CloseButton" template="SSF_Menu/CloseButtonTemplate"></Frame>
		<Visible val="false"/>
	</Frame>





<!-- Achievement Window -->
	<Constant name="AcvColorLocked" val="135,135,135"/>
	<Constant name="AcvColor0" val="0,178,0"/>
	<Constant name="AcvColor1" val="178,178,0"/>
	<Constant name="AcvColor2" val="178,0,0"/>
	<Constant name="AcvColor3" val="178,89,0"/>
	<Constant name="AcvColor4" val="0,0,178"/>
	<Constant name="AcvColor5" val="0,178,178"/>
	<Constant name="AcvColor6" val="178,0,178"/>
	<Constant name="AchievementIconSpacing" val="6"/>
	<Constant name="AchievementIconSize" val="76"/>
	<Constant name="AchievementIconBorderSize" val="88"/>

	<Frame type="Frame" name="AchievementIconNormalTemplate">
		<Width val="#AchievementIconBorderSize"/>
		<Height val="#AchievementIconBorderSize"/>

		<Frame type="Image" name="Notification" template="SSF_Menu/NotificationImageInAchievementsTemplate"></Frame>

		<Animation name="NotificationAnim">
			<Event event="Notification" action="Reset,Play" frame="$this"/>
			<Controller type="State" end="Pause" frame="$this" stateGroup="NotificationState">
				<Key type="Identifier" time="0.0" value="True"/>
			</Controller>
		</Animation>

		<StateGroup name="NotificationState">
			<DefaultState val="False"/>
			<State name="False">
				<Action type="SetProperty" frame="$this/Notification" Visible="False"/>
			</State>
			<State name="True">
				<Action type="SetProperty" frame="$this/Notification" Visible="True"/>
			</State>
		</StateGroup>

		<Frame type="Image" name="Icon">
			<Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
			<Texture val="Assets/Textures/ui_ingame_help_techtree_questionmark.dds"/>
			<Width val="#AchievementIconSize"/>
		</Frame>
		<Frame type="Image" name="Border">
			<Anchor relative="$parent"/>
			<Texture val="Assets/Icons/Icon_Border/Icon_Border_White.tga"/>
		</Frame>
		<StateGroup name="Stage">
			<DefaultState val="0"/>
			<State name="0">
				<When type="AnimationState" frame="$this" operator="Equal" Stage="0"/>
				<Action type="SetProperty" frame="$this/Border" color="#AcvColorLocked"/>
			</State>
			<State name="1">
				<When type="AnimationState" frame="$this" operator="Equal" Stage="1"/>
				<Action type="SetProperty" frame="$this/Border" color="#AcvColor0"/>
			</State>
			<State name="2">
				<When type="AnimationState" frame="$this" operator="Equal" Stage="2"/>
				<Action type="SetProperty" frame="$this/Border" color="#AcvColor1"/>
			</State>
			<State name="3">
				<When type="AnimationState" frame="$this" operator="Equal" Stage="3"/>
				<Action type="SetProperty" frame="$this/Border" color="#AcvColor2"/>
			</State>
			<State name="4">
				<When type="AnimationState" frame="$this" operator="Equal" Stage="4"/>
				<Action type="SetProperty" frame="$this/Border" color="#AcvColor3"/>
			</State>
			<State name="5">
				<When type="AnimationState" frame="$this" operator="Equal" Stage="5"/>
				<Action type="SetProperty" frame="$this/Border" color="#AcvColor4"/>
			</State>
			<State name="6">
				<When type="AnimationState" frame="$this" operator="Equal" Stage="6"/>
				<Action type="SetProperty" frame="$this/Border" color="#AcvColor5"/>
			</State>
			<State name="7">
				<When type="AnimationState" frame="$this" operator="Equal" Stage="7"/>
				<Action type="SetProperty" frame="$this/Border" color="#AcvColor6"/>
			</State>
		</StateGroup>
	</Frame>
	<Frame type="Frame" name="AchievementIconCutsTemplate">
		<Width val="#AchievementIconBorderSize"/>
		<Height val="#AchievementIconBorderSize"/>

		<Frame type="Image" name="Notification" template="SSF_Menu/NotificationImageInAchievementsTemplate"></Frame>

		<Animation name="NotificationAnim">
			<Event event="Notification" action="Reset,Play" frame="$this"/>
			<Controller type="State" end="Pause" frame="$this" stateGroup="NotificationState">
				<Key type="Identifier" time="0.0" value="True"/>
			</Controller>
		</Animation>

		<StateGroup name="NotificationState">
			<DefaultState val="False"/>
			<State name="False">
				<Action type="SetProperty" frame="$this/Notification" Visible="False"/>
			</State>
			<State name="True">
				<Action type="SetProperty" frame="$this/Notification" Visible="True"/>
			</State>
		</StateGroup>


		<Frame type="Image" name="Icon">
			<Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
			<Texture val="Assets/Textures/ui_ingame_help_techtree_questionmark.dds"/>
			<Width val="#AchievementIconSize"/>
		</Frame>
		<Frame type="Image" name="Border0">
			<Anchor relative="$parent"/>
			<Texture val="Assets/Icons/Icon_Border/Icon_Border_Segemented_TopRight_White.tga"/>
			<StateGroup name="Unlocked">
				<DefaultState val="Locked"/>
				<State name="Unlocked">
					<When type="AnimationState" frame="$this" operator="Equal" Unlocked="True"/>
					<Action type="SetProperty" frame="$this" Color="#AcvColor0"/>
				</State>
				<State name="Locked">
					<Action type="SetProperty" frame="$this" Color="#AcvColorLocked"/>
				</State>
			</StateGroup>
		</Frame>
		<Frame type="Image" name="Border1">
			<Anchor relative="$parent"/>
			<Texture val="Assets/Icons/Icon_Border/Icon_Border_Segemented_Right_White.tga"/>
			<StateGroup name="Unlocked">
