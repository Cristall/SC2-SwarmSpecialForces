		<Frame type="Image" name="BackgroundImage" template="SSF_Menu/BackgroundImageTemplate"></Frame>
		<Frame type="Button" name="CloseButton" template="SSF_Menu/CloseButtonTemplate"></Frame>
		
		<Frame type="Label" name="AutoHeroSelectionLabel" template="SSF_Menu/LabelTemplate">
			<Anchor side="Top" relative="$parent" pos="Min" offset="#Border"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="#Border"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="#BorderNegative"/>
			<Height val="50"/>
			<Style val="aaDialogItemStyleLeftBottom"/>
			<Text val="@Trig/Options/AutoHeroSelection"/>
		</Frame>

		<Frame type="CheckBox" name="AutoHeroSelectionCheckbox" template="SSF_Menu/OptionsCheckboxTemplate">
			<Anchor side="Top" relative="$parent/AutoHeroSelectionLabel" pos="Max" offset="#OptionsLabelGap"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="#Border"/>
			<Tooltip val="@Trig/Options/AutoHeroSelectionTooltip"/>
		</Frame>

		<Frame type="Pulldown" name="AutoHeroSelectionPulldown" template="SSF_Menu/PulldownTemplate">
			<Width val="145"/>
			<Anchor side="Left" relative="$parent/AutoHeroSelectionCheckbox" pos="Max" offset="#OptionsCheckboxGap"/>
